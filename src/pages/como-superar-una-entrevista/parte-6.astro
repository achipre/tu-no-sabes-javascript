          }
          int[] appendToNew(int[] array, int value) &#123 
            // copy all elements over to new array 
            int[] bigger= new int[array.length + 1]; 
            for (int i = 0; i &#60 array.length; i++) &#123
              bigger[i] = array[i];
            }
            // add new element 
            bigger[bigger.length - 1] = value
            return bigger;
